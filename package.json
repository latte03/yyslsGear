{
  "name": "vue3-template",
  "version": "0.1.3",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "concurrently \"pnpm:type-check\" \"pnpm:build-only\"",
    "preview": "vite preview",
    "test": "vitest",
    "test:unit": "vitest --environment jsdom --root src/",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "eslint-viewer": "@eslint/config-inspector",
    "lint": "eslint . --fix",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "cz": "git add . && cz",
    "bumpp": "bumpp"
  },
  "dependencies": {
    "@eslint/config-inspector": "^1.4.2",
    "@unocss/reset": "^66.6.0",
    "@vue-macros/reactivity-transform": "^3.1.2",
    "@vueuse/core": "^14.1.0",
    "@vueuse/head": "^2.0.0",
    "alova": "^3.4.1",
    "axios": "^1.13.2",
    "bumpp": "^10.4.0",
    "fast-querystring": "^1.1.2",
    "lodash-es": "^4.17.22",
    "nprogress-v2": "^1.1.10",
    "pinia": "^3.0.4",
    "tdesign-icons-vue-next": "^0.4.1",
    "tdesign-vue-next": "^1.18.0",
    "unocss": "^66.6.0",
    "vue": "^3.5.26",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@plugin-web-update-notification/vite": "^2.0.2",
    "@sxzz/eslint-config": "^7.4.5",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^25.0.9",
    "@unocss/preset-rem-to-px": "^66.6.0",
    "@unocss/transformer-directives": "^66.6.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitejs/plugin-vue-jsx": "^5.1.3",
    "@vue-macros/volar": "^3.1.2",
    "@vue/runtime-core": "^3.5.26",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "commitizen": "^4.3.1",
    "commitlint": "^20.3.1",
    "concurrently": "^9.2.1",
    "cz-customizable": "^7.5.1",
    "eslint": "^9.39.2",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "jsdom": "^27.4.0",
    "less": "^4.5.1",
    "pnpm-patch-i": "^0.5.6",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.1",
    "postcss-less": "^6.0.0",
    "prettier": "^3.8.0",
    "stylelint": "^16.23.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^7.4.0",
    "stylelint-config-recommended": "^17.0.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-declaration-strict-value": "^1.10.11",
    "stylelint-order": "^7.0.1",
    "terser": "^5.46.0",
    "typescript": "^5.9.3",
    "unplugin-auto-import": "^21.0.0",
    "unplugin-compression": "^0.0.6",
    "unplugin-info": "^1.2.4",
    "unplugin-svg-component": "^0.12.4",
    "unplugin-turbo-console": "^2.3.0",
    "unplugin-unused": "^0.5.6",
    "unplugin-vue-components": "^31.0.0",
    "unplugin-vue-inspector": "^2.3.2",
    "unplugin-vue-router": "^0.19.2",
    "vite": "8.0.0-beta.8",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vite-plugin-vue-meta-layouts": "^0.6.1",
    "vite-tsconfig-paths": "^6.0.4",
    "vitest": "^4.0.17",
    "volar-plugin-vue-router": "^0.2.0",
    "vue-macros": "3.1.2",
    "vue-tsc": "^3.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "config": {
    "cz-customizable": {
      "config": "./.cz-config.cjs"
    },
    "commitizen": {
      "path": "cz-customizable"
    }
  },
  "pnpm": {
    "patchedDependencies": {
      "nprogress-v2": "patches/nprogress-v2.patch"
    },
    "overrides": {
      "vite": "8.0.0-beta.7"
    }
  }
}
